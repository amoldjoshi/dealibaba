<div class = "container">
  <div class="newform-box">
    <div class = "container">
      <p>
        <strong>Productpicture:</strong>
        <%- if @product.picture.attached? %>
            <%= image_tag url_for(@product.picture) %>
        <%- end %>
      </p>

      <p>
        <strong>Productid:</strong>
        <%= @product.productid %>
      </p>

      <p>
        <strong>Productname:</strong>
        <%= @product.productname %>
      </p>

      <p>
        <strong>Producturl:</strong>
        <%= @product.producturl %>
      </p>

      <p>
        <strong>Producttype:</strong>
        <%= @product.producttype %>
      </p>

      <p>
        <strong>Productdescription:</strong>
        <%= @product.productdescription %>
      </p>

      <p>
        <strong>ProductResearch:</strong>
        <%= @product.productresearch %>
      </p>

      <p>
        <strong>Productbutton:</strong>
        <%= @product.productbutton %>
      </p>

      <p>
        <strong>Productprice:</strong>
        <%= @product.productprice %>
      </p>

      <p>
        <strong>Productvisible:</strong>
        <%= @product.productvisible %>
      </p>
      
      <p>
        <strong>Productcategory:</strong>
        <%= @product.productcategory %>
      </p>

    </div><!--end container-->
<div>
    <%= link_to 'Edit', edit_product_path(@product) %> |
    <%= link_to 'Back', products_path %>
</div>
    
  </div><!--end newform-box input-->


</div>








